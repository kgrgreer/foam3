{
  "@id": "workflow",
  "@uri": "http://www.ingenico.com/schemas/web/workflow/1_0",
  "ops": [
    {
      "@id": "import",
      "minify": false,
      "path": "src/net/nanopay/merchant"
    },
    {
      "@id": "packaging",
      "mode": "AMS"
    },
    {
      "@id": "sign",
      "certificateType": "VERTICAL_WEB",
      "mode": "Mock-Up"
    }
  ],
  "targetPath": "dist/"
}